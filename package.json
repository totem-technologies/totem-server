{
  "name": "theme",
  "version": "3.5.0",
  "description": "",
  "type": "module",
  "scripts": {
    "build:atcb": "bun build node_modules/add-to-calendar-button/dist/atcb-no-pro.js --drop=console --minify --outfile=totem/static/js/atcb.min.js && perl -pi -e s,add-to-calendar-pro.com,totem.org,g totem/static/js/atcb.min.js",
    "build:js": "bun run build.ts",
    "build:tailwind": "NODE_ENV=production bun run tw -- --minify",
    "build": "concurrently \"bun run build:tailwind\" \"bun run build:js\"",
    "dev": "concurrently --kill-others-on-fail \"bun run dev:js\" \"bun run dev:tailwind\" \"bun run build:atcb\"",
    "livereload": "livereload ./totem/ -e 'html,css,js,py,mjml' --wait 200",
    "openapi-ts": "openapi-ts",
    "start": "bun run dev",
    "tw": "tailwindcss -i ./assets/css/styles.css -o ./totem/static/js/bundles/styles.css",
    "dev:js": "bun run build.ts -- dev",
    "dev:tailwind": "NODE_ENV=development bun run tw",
    "test": "vitest",
    "test:ci": "vitest run"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "engines": {
    "bun": "<2"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.10",
    "@eslint/js": "^9.39.2",
    "@hey-api/openapi-ts": "^0.89.2",
    "@solidjs/testing-library": "^0.8.10",
    "@tailwindcss/cli": "^4.1.18",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tailwindcss/typography": "^0.5.19",
    "@tanstack/eslint-plugin-query": "^5.91.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/bun": "^1.3.5",
    "@types/js-cookie": "^3.0.6",
    "bun-plugin-solid": "^1.0.0",
    "concurrently": "^9.2.1",
    "daisyui": "^5.5.14",
    "eslint": "^9.39.2",
    "eslint-plugin-solid": "^0.14.5",
    "eslint-plugin-tailwindcss": "^3.18.2",
    "jsdom": "^27.1.0",
    "livereload": "^0.10.3",
    "postcss": "^8.5.6",
    "postcss-import": "^16.1.1",
    "postcss-nested": "^7.0.2",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.7.4",
    "prettier-plugin-jinja-template": "^2.1.0",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.1",
    "vite-plugin-solid": "^2.11.10",
    "vite-tsconfig-paths": "^6.0.3",
    "vitest": "^4.0.16"
  },
  "dependencies": {
    "@leeoniya/ufuzzy": "^1.0.19",
    "@rnwonder/solid-date-picker": "^1.5.0",
    "@solid-primitives/intersection-observer": "^2.2.2",
    "@solid-primitives/keyboard": "^1.3.3",
    "@solid-primitives/media": "^2.3.3",
    "@solid-primitives/refs": "^1.1.2",
    "@tanstack/solid-query": "^5.90.15",
    "@totem.org/solid-boring-avatars": "^0.0.2",
    "@zootools/email-spell-checker": "^1.12.0",
    "add-to-calendar-button": "^2.13.4",
    "js-cookie": "^3.0.5",
    "solid-element": "^1.9.1",
    "solid-icons": "^1.1.0",
    "solid-js": "^1.9.10",
    "tippy.js": "^6.3.7"
  }
}
